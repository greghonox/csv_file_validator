{
  "file_metadata": {
    "value_separator": "|",
    "row_terminator": "\n",
    "value_quote_char": "\"",
    "file_has_header": false
  },
  "file_validation_rules": {
    "file_name_file_mask": "DB_3388",
    "file_extension": "TXT",
    "file_size_range": [
      0,
      5
    ],
    "file_row_count_range": [
      0,
      10000
    ]
  },

