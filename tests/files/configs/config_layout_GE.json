      ]
    },

    "1": {
      "allow_data_type": "str"
    },

    "2": {
      "allow_data_type": "str"
    },

    "3": {
      "allow_data_type": "str",
      "allow_regex":"[0-9,]"
    },

    "4": {
      "allow_data_type": "int",
      "allow_int_value_range":[3, 24]
    },

    "5": {
      "allow_null_value": true,
      "allow_data_type": "int",
      "allow_int_value_range":[12, 36]
    },

    "6": {
      "allow_null_value" : true,
      "allow_data_type": "str",
      "allow_regex":"[0-9,]"
    },

    "7": {
